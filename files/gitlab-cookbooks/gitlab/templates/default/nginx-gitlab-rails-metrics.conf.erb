location /rails-metrics {
  proxy_cache off;
  proxy_http_version 1.0;
  proxy_pass  http://gitlab-workhorse/-/metrics;
  <% @options.each do |key, value|
    if value.is_a?(String) -%>
      <%= key %> <%= value %>;
    <% elsif value.is_a?(Array)
      value.each do |v| -%>
        <%= key %> <%= v %>;
      <% end %>
    <% end %>
  <% end %>
}
